<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite name="webapi-usecase-tests">
    <set name="DOM, Forms and Styles">
      <testcase component="usecase" execution_type="manual" id="Jsenhance" purpose="Jsenhance Test">
      </testcase>
    </set>
    <set name="Communication">
      <testcase component="usecase" execution_type="manual" id="RawSockets" purpose="Raw Sockets Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="WebSocket" purpose="WebSocket Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="NBS" purpose="NBS Test">
        <capability name="nbs"/>
      </testcase>
    </set>
    <set name="Storage">
      <testcase component="usecase" execution_type="manual" id="TizenFileSystem" purpose="Tizen File System Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="FileSystem" purpose="File System Test">
      </testcase>
    </set>
    <set name="Security">
      <testcase component="usecase" execution_type="manual" id="CSP" purpose="CSP Test" subcase="9">
      </testcase>
    </set>
    <set name="Supplementary">
      <testcase component="usecase" execution_type="manual" id="Viewport" purpose="Viewport Test">
      </testcase>
    </set>
    <set name="Application">
      <testcase component="usecase" execution_type="manual" id="ApplicationManager" purpose="ApplicationManager Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Package" purpose="Package Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="DataControl" purpose="DataControl Test">
      </testcase>
    </set>
    <set name="Social">
      <testcase component="usecase" execution_type="manual" id="Bookmark" purpose="Bookmark Test">
      </testcase>
    </set>
    <set name="I/O">
      <testcase component="usecase" execution_type="manual" id="MessagePort" purpose="MessagePort Test">
      </testcase>
    </set>
    <set name="Content">
      <testcase component="usecase" execution_type="manual" id="Download" purpose="Download Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Content" purpose="Content Test">
      </testcase>
    </set>
    <set name="System">
      <testcase component="usecase" execution_type="manual" id="SystemInfo" purpose="SystemInfo Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="SystemSettingScreen" purpose="SystemSetting Screen Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="SystemSettingIncomingCall" purpose="SystemSetting IncomingCall Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Time" purpose="Time Test">
      </testcase>
    </set>
    <set name="Others">
      <testcase component="usecase" execution_type="manual" id="AudioSystem" purpose="AudioSystem Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="SSO" purpose="SSO Test">
      </testcase>
      <testcase component="usecase" execution_type="manual" id="Tizen" purpose="Tizen Test">
      </testcase>
    </set>
  </suite>
</test_definition>
