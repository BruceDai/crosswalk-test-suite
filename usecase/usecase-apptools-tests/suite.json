{
  "pkg-blacklist": [
    "pack.py",
    "testcase.xsl",
    "testresult.xsl",
    "tests.css",
    "icon.png",
    "manifest.json",
    "suite.json",
    "inst.*"
  ],
  "pkg-list": {
    "apk": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "README.md": "README.md",
        "samples-android": "samples",
        "testapp": "testapp",
        "inst.apk.py": "inst.apk.py",
        "tests.android.xml": "tests.xml",
        "tools": "tools"
      },
      "subapp-list": {
        ".": {
          "app-name": "usecase-apptools-tests",
          "apk-common-opts": "--enable-remote-debugging",
          "blacklist": [
            "*",
            ".git"
          ],
          "copylist": {
            "PACK-TOOL-ROOT/bootstrap-fw": ".",
            "icon.png": "icon.png",
            "testapp": "testapp",
            "samples-android": "samples",
            "steps": "steps",
            "tests.android.xml": "tests.xml"
          }
        },
        "samples-android/ManifestVersionName/res": {
          "apk-type": "MANIFEST",
          "copylist": {
            "../../../testapp/start_url/index.html": "index.html"
          }
        }
      }
    },
    "deb": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "README.md": "README.md",
        "samples-deb": "samples",
        "tests.deb.xml": "tests.xml",
        "tools": "tools"
      },
      "pkg-app": {}
    },
    "ios": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "README.md": "README.md",
        "samples-ios": "samples",
        "tests.ios.xml": "tests.xml",
        "tools": "tools"
      },
      "pkg-app": {}
    },
    "msi": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "README.md": "README.md",
        "samples-windows": "samples",
        "tests.windows.xml": "tests.xml",
        "tools": "tools"
      },
      "pkg-app": {}
    }
  },
  "pkg-name": "usecase-apptools-tests"
}
