{
  "pkg-blacklist": [
    "pack.py",
    "icon.png",
    "manifest.json",
    "suite.json",
    "*-py",
    "opt",
    "*.zip",
    "inst.*"
  ],
  "pkg-list": {
    "apk": {
      "blacklist": [],
      "copylist": {
        "inst.apk.py": "inst.py",
        "tests.android.xml": "tests.xml",
        "tests.full.xml": "tests.full.xml"
      },
      "subapp-list": {
        ".": {
          "apk-common-opts": "--enable-remote-debugging",
          "app-name": "usecase-extension-android-tests",
          "blacklist": [
            "manifest.json",
            "opt"
          ],
          "copylist": {
            "PACK-TOOL-ROOT/bootstrap-fw": ".",
            "steps": "steps",
            "tests.android.xml": "tests.xml"
          },
          "install-path": "."
        },
        "samples/XWalkEchoApp": {
          "apk-ext-src": "samples/XWalkEchoExtension",
          "apk-ext-opt": "samples/XWalkEchoExtension/echoextension",
          "app-name": "xwalk_echo_app",
          "app-type": "EXTENSION",
          "apk-type": "MANIFEST",
          "install-path": "."
        },
        "samples/XWalkContactApp": {
          "apk-ext-src": "samples/XWalkContactExtension",
          "apk-ext-opt": "samples/XWalkContactExtension/contactextension",
          "app-name": "xwalk_contact_app",
          "app-type": "EXTENSION",
          "apk-type": "MANIFEST",
          "install-path": "."
        },
        "samples/XWalkEchoJsStubGenApp": {
          "apk-ext-src": "samples/XWalkEchoJsStubGenExtension",
          "apk-ext-opt": "samples/XWalkEchoJsStubGenExtension/echoJsStubGenExtension",
          "app-name": "xwalk_echo_jsstubgen_app",
          "app-type": "EXTENSION",
          "apk-type": "MANIFEST",
          "install-path": "."
        },
        "samples/XWalkConstructorJsStubGenApp": {
          "apk-ext-src": "samples/XWalkConstructorJsStubGenExtension",
          "apk-ext-opt": "samples/XWalkConstructorJsStubGenExtension/constructorJsStubGen",
          "app-name": "xwalk_constructor_jsstubgen_app",
          "app-type": "EXTENSION",
          "apk-type": "MANIFEST",
          "install-path": "."
        },
        "samples/XWalkJsStubAppByBMI": {
          "apk-ext-src": "samples/XWalkJsStubExtensionByBMI",
          "apk-ext-opt": "samples/XWalkJsStubExtensionByBMI/jsStubExtensionByBMI",
          "app-name": "xwalk_jsstub_app_by_bmi",
          "app-type": "EXTENSION",
          "apk-type": "MANIFEST",
          "install-path": "."
        },
        "samples/ExtensionPermission/res": {
          "apk-ext-opt": "samples/ExtensionPermission/res/contactextension",
          "app-name": "extension_permission",
          "install-path": "."
        }
      }
    }
  },
  "pkg-name": "usecase-extension-android-tests"
}
