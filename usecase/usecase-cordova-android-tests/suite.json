{
    "pkg-blacklist": [
        "pack.py",
        "testcase.xsl",
        "testresult.xsl",
        "tests.css",
        "icon.png",
        "suite.json",
        "inst.apk.py",
        "config.xml",
        "manifest.json"
    ],
    "pkg-list": {
        "cordova": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "inst.apk.py": "inst.py",
                "tests.xml": "tests.xml"
            },
            "subapp-list": {
                ".": {
                    "app-name": "usecase-cordova-android-tests",
                    "install-path": "./",
                    "copylist": {
                        "tests/cordova-incl.js": "cordova-incl.js"
                    }
                }
            }
        }
    },
    "pkg-name": "usecase-cordova-android-tests"
}
