{
    "pkg-blacklist": [
        "pack.py",
        "icon.png",
        "suite.json",
        "inst.apk.py",
        "manifest.json"
    ],
    "pkg-list": {
        "cordova": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "inst.apk.py": "inst.py",
                "tests.xml": "tests.xml",
                "samples/CordovaPackage3.6/res/test_embedded.py": "res/CordovaPackage/test_embedded.py",
                "samples/CordovaPackage3.6/res/test_shared.py": "res/CordovaPackage/test_shared.py",
                "samples/Database/upstream/database_upstream_3.6.apk": "database_upstream.apk"
            },
            "subapp-list": {
                ".": {
                    "app-name": "usecase-cordova-android-tests",
                    "install-path": "./",
                    "copylist": {
                        "samples/cordova-incl.js": "cordova-incl.js",
                        "steps": "steps",
                        "samples/AdMob3.6": "samples/AdMob",
                        "thirdparty_plugins/cordova-admob-3.6": "PACK-TOOL-ROOT/thirdparty_plugins/cordova-admob-3.6",
                        "thirdparty_plugins/cordova-screenshot-3.6": "PACK-TOOL-ROOT/thirdparty_plugins/cordova-screenshot",
                        "PACK-TOOL-ROOT/bootstrap-fw": ".",
                        "samples/CordovaPackage3.6": "samples/CordovaPackage",
                        "steps/CordovaPackage3.6": "steps/CordovaPackage",
                        "samples/StatusBar3.6": "samples/StatusBar",
                        "samples/Database/index.3.6.html": "samples/Database/index.html"
                    }
                },
                "samples/Database/res/database_source": {
                    "app-name": "database",
                    "install-path": "./",
                    "copylist": {
                        "PACK-TOOL-ROOT/bootstrap-fw/js/jquery-2.1.3.min.js": "./js/jquery.js"
                    }
                }
            }
        },
        "cordova4.x": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "inst.apk.py": "inst.py",
                "tests.xml": "tests.xml",
                "samples/Database/res": "res/Database",
                "PACK-TOOL-ROOT/bootstrap-fw/js/jquery-2.1.3.min.js": "res/Database/database_source/js/jquery.js"
            },
            "subapp-list": {
                ".": {
                    "app-name": "usecase-cordova-android-tests",
                    "install-path": "./",
                    "copylist": {
                        "samples/cordova-incl.js": "cordova-incl.js",
                        "steps": "steps",
                        "samples/AdMob4.x": "samples/AdMob",
                        "thirdparty_plugins/cordova-admob-4.x": "PACK-TOOL-ROOT/thirdparty_plugins/cordova-admob-4.x",
                        "thirdparty_plugins/cordova-screenshot-4.x": "PACK-TOOL-ROOT/thirdparty_plugins/cordova-screenshot",
                        "PACK-TOOL-ROOT/bootstrap-fw": ".",
                        "samples/CordovaPackage4.x": "samples/CordovaPackage",
                        "steps/CordovaPackage4.x": "steps/CordovaPackage",
                        "samples/StatusBar4.x": "samples/StatusBar",
                        "samples/Database/index.4.x.html": "samples/Database/index.html"
                    }
                },
                "samples/Database/res/database_source": {
                    "app-name": "database",
                    "install-path": "./",
                    "copylist": {
                        "PACK-TOOL-ROOT/bootstrap-fw/js/jquery-2.1.3.min.js": "./js/jquery.js"
                    }
                }
            }
        }
    },
    "pkg-name": "usecase-cordova-android-tests"
}
