{
    "pkg-blacklist": [
        "pack.py",
        "testcase.xsl",
        "testresult.xsl",
        "tests.css",
        "icon.png",
        "manifest.json",
        "suite.json",
        "inst.*"
    ],
    "pkg-list": {
        "deb": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "sampleapp-deb": "sampleapp",
                "tools": "tools",
                "tests.deb.xml": "tests.xml",
                "README": "README"
            },
            "pkg-app": {
                "copylist": {
                    "tests.deb.xml": "tests.xml",
                    "PACK-TOOL-ROOT/resources/testharness": "resources",
                    "PACK-TOOL-ROOT/resources/webrunner": "webrunner"
                }
            }
        },
        "apk": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "sampleapp-android": "sampleapp",
                "tools": "tools",
                "tests.android.xml": "tests.xml",
                "README": "README"
            },
            "pkg-app": {
                "copylist": {
                    "tests.android.xml": "tests.xml",
                    "PACK-TOOL-ROOT/resources/testharness": "resources",
                    "PACK-TOOL-ROOT/resources/webrunner": "webrunner"
                }
            }
        },
        "ios": {
            "blacklist": [
                "*"
            ],
            "copylist": {
                "sampleapp-ios": "sampleapp",
                "tools": "tools",
                "tests.ios.xml": "tests.xml",
                "README": "README"
            },
            "pkg-app": {
                "copylist": {
                    "tests.ios.xml": "tests.xml",
