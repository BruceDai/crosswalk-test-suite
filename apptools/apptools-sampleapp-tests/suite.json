{
  "pkg-blacklist": [
    "pack.py",
    "testcase.xsl",
    "testresult.xsl",
    "tests.css",
    "icon.png",
    "manifest.json",
    "suite.json",
    "inst.*"
  ],
  "pkg-list": {
    "apk": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "README.md": "README.md",
        "sampleapp-android": "sampleapp",
        "tests.android.xml": "tests.xml",
        "tools": "tools"
      },
      "pkg-app": {
        "copylist": {
          "PACK-TOOL-ROOT/resources/testharness": "resources",
          "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
          "tests.android.xml": "tests.xml"
        }
      }
    },
    "deb": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "README.md": "README.md",
        "sampleapp-deb": "sampleapp",
        "tests.deb.xml": "tests.xml",
        "tools": "tools"
      },
      "pkg-app": {
        "copylist": {
          "PACK-TOOL-ROOT/resources/testharness": "resources",
          "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
          "tests.deb.xml": "tests.xml"
        }
      }
    },
    "ios": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "README.md": "README.md",
        "sampleapp-ios": "sampleapp",
        "tests.ios.xml": "tests.xml",
        "tools": "tools"
      },
      "pkg-app": {
        "copylist": {
          "PACK-TOOL-ROOT/resources/testharness": "resources",
          "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
          "tests.ios.xml": "tests.xml"
        }
      }
    },
    "msi": {
      "blacklist": [
        "*"
      ],
      "copylist": {
        "README.md": "README.md",
        "sampleapp-windows": "sampleapp",
        "tests.windows.xml": "tests.xml",
        "tools": "tools"
      },
      "pkg-app": {
        "copylist": {
          "PACK-TOOL-ROOT/resources/testharness": "resources",
          "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
          "tests.windows.xml": "tests.xml"
        }
      }
