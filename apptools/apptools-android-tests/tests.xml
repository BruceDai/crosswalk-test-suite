          <test_script_entry>/opt/apptools-android-tests/apptools/create_no_sdk.py</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk App Tools/CLI" execution_type="auto" id="Crosswalk_create_local" purpose="Android - Validate if App Tool can work well with create and crosswalk option" subcase="3">
