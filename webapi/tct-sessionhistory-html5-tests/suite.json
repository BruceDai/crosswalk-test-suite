            "../sessionhistory/support": "sessionhistory/support"
          }
        }
      }
    },
    "apk-aio, cordova-aio": {
      "blacklist": [],
      "copylist": {
        "PACK-TOOL-ROOT/atip/tests/environment.py": "HOST_RESOURCES/html5-sessionhistory-app/testscripts/environment.py",
        "PACK-TOOL-ROOT/resources/testharness": "resources",
