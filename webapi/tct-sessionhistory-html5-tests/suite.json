        "sessionhistory/support": "sessionhistory/bdd/support"
      },
      "subapp-list": {
        "html5-sessionhistory-app": {
          "apk-common-opts": "--enable-remote-debugging",
          "apk-icon-opt": "",
          "copylist": {
            "../sessionhistory/bdd": "sessionhistory",
            "../sessionhistory/support": "sessionhistory/support"
          }
        }
      }
    },
    "wgt": {
      "blacklist": [],
      "copylist": {
        "PACK-TOOL-ROOT/resources/testharness": "resources",
        "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
        "inst.wgt.py": "inst.py",
        "sessionhistory/w3c/cgi": "sessionhistory/w3c",
        "tests.full.xml": "tests.full.xml",
        "tests.xml": "tests.xml"
      },
      "pkg-app": {
        "blacklist": [],
        "hosted-app": "true"
      }
    },
    "xpk": {
      "blacklist": [],
      "copylist": {
        "PACK-TOOL-ROOT/resources/testharness": "resources",
        "PACK-TOOL-ROOT/resources/webrunner": "webrunner",
        "inst.xpk.py": "inst.py",
        "sessionhistory/w3c/cgi": "sessionhistory/w3c",
        "tests.full.xml": "tests.full.xml",
        "tests.xml": "tests.xml"
      },
      "pkg-app": {
        "blacklist": [],
        "hosted-app": "true"
      }
    }
  },
  "pkg-name": "tct-sessionhistory-html5-tests"
