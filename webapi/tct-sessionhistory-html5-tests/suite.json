        "sessionhistory/w3c/cgi": "sessionhistory/w3c",
        "tests.full.xml": "tests.full.xml",
        "tests.xml": "tests.xml"
      },
      "pkg-app": {
        "blacklist": [],
        "copylist": {
          "sessionhistory/support": "sessionhistory/bdd/support"
        },
        "hosted-app": "true"
      },
      "subapp-list": {
        "html5-sessionhistory-app": {
          "apk-common-opts": "--enable-remote-debugging",
          "apk-icon-opt": "",
          "copylist": {
