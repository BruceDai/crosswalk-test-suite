{
  "pkg-blacklist": [
    "config.xml",
    "config.xml.*",
    "pack.py",
    "testcase.xsl",
    "testresult.xsl",
    "tests.css",
    "icon.png",
    "manifest.json",
    "suite.json",
    "*-py",
    "inst.*"
  ],
  "pkg-list": {
    "apk,cordova": {
      "blacklist": [],
      "copylist": {
        "PACK-TOOL-ROOT/atip/tests/environment.py": "html5-sessionhistory-app/testscripts/environment.py",
        "PACK-TOOL-ROOT/resources/bdd/bddrunner": "bddrunner",
        "PACK-TOOL-ROOT/resources/testharness": "resources",
