<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="W3C/HTML5 APIs" name="webapi-presentation-w3c-tests">
    <set name="presentation" type="js">
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="Presentation_basic" purpose="Test checks that Presentation API is present" subcase="4">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/Presentation_basic.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="Presentation_defaultRequest" purpose="Test checks that defaultRequest returns the default presentation request">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/Presentation_defaultRequest.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationRequest_start" purpose="Test checks that start method returns a new PresentationConnection">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationRequest_start.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationRequest_start_InvalidAccessError" purpose="Test checks that PresentationRequest.start() requires user gesture">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationRequest_start_InvalidAccessError.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationRequest_start_NotFoundError" purpose="Test checks that PresentationRequest.start() throws a 'NotFoundError' exception when no available presentation displays">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationRequest_start_NotFoundError.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationRequest_denied" purpose="Test checks that PresentationRequest.start() throws a AbortError exception when the user denies permission">
        <description>
          <pre_condition>
            Turn on 'Simulate Secondary Display' by 'Setting' app.
          </pre_condition>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationRequest_denied.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationRequest_reconnect" purpose="Test checks that PresentationRequest.reconnect() can reconnect the presentation displays with known presentationId">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationRequest_reconnect.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationRequest_reconnect_TypeError" purpose="Test checks that PresentationRequest.reconnect() rejects with a 'TypeError' exception when missing argument">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationRequest_reconnect_TypeError.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationRequest_reconnect_NotFoundError" purpose="Test checks that PresentationRequest.reconnect() rejects with a 'NotFoundError' exception when the known connection's state is terminated">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationRequest_reconnect_NotFoundError.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationRequest_onconnectionavailable" purpose="Test checks that onconnectionavailable is fired when the user agent starts a presentation successfully">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationRequest_onconnectionavailable.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationRequest_miss_argument" purpose="Test checks that onconnectionavailable is fired when the user agent starts a presentation successfully">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationRequest_miss_argument.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationAvailability_basic" purpose="Test checks that the PresentationAvailability API is present">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationAvailability_basic.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="manual" id="Prensentation_availability" purpose="Test checks that user agent monitors availability of presentation displays">
        <description>
          <pre_condition>
            Turn on 'Simulate Secondary Display' by 'Setting' app.
          </pre_condition>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/Prensentation_availability-manual.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnection_basic" purpose="Test checks that PresentationConnection API is present">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnection_basic.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnection_send_string" purpose="Test checks that PresentationConnection can send string data">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnection_send_string.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnection_send_blob" purpose="Test checks that PresentationConnection can send blob data">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnection_send_blob.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnection_send_arraybuffer" purpose="Test checks that PresentationConnection can send arraybuffer data">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnection_send_arraybuffer.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnection_send_arraybufferview" purpose="Test checks that PresentationConnection can send arraybufferview data">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnection_send_arraybufferview.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnection_onconnected" purpose="Test checks that onconnected is fired when the user agent connect the presentationConnection to receiving browsing context successfully">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnection_onconnected.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnection_onclosed" purpose="Test checks that onclosed event is fired when close() method is called">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnection_onclosed.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnection_onmessage" purpose="Test checks that onmessage event is fired when user agent receive message through a PresentationConnection">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnection_onmessage.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnection_onterminated" purpose="Test checks that onterminated eventhandler is fired when terminate() is called">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnection_onterminated.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk APIs/Experimental/Screen Presentation APIs" execution_type="auto" id="PresentationConnectionAvailableEvent" purpose="Test checks that constructing connectionavailable event" subcase="2">
        <description>
          <test_script_entry>/opt/webapi-presentation-w3c-tests/presentation/PresentationConnectionAvailableEvent.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
