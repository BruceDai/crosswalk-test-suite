<test_definition>
  <suite category="Crosswalk WRT" name="wrt-packagemgtmanu-tizen-tests">
    <set name="packagemgtmanu" type="js">
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_App_Icon" priority="P1" purpose="Validate if web app icon display normally" status="designed" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure there some webapps are available.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_App_Icon.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_App_Icon_Launcher" priority="P1" purpose="Validate if Crosswalk support that web app could be launched from app launcher successfully" status="designed" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure there some webapps are available.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_App_Icon_Launcher.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_WebApp_Uninstall" priority="P1" purpose="Validate if Crosswalk support web apps could be uninstalled successfully" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_WebApp_Uninstall.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_WebApp_Launch" priority="P0" purpose="Validate Crosswalk support that web app could be launched successfully" status="designed" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk application is launched
            2.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_WebApp_Launch.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_Wiget_Update_SameVersion" priority="P1" purpose="Validate if Crosswalk support to Update the installed Widget" status="designed" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed;
            2.Make sure the wgt tct-2dtransforms-css3-tests.wgt is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_Wiget_Update_SameVersion.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_Remote_Debugging_Connection" priority="P1" purpose="Validate Crosswalk could debug by remote host" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk is installed.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_Remote_Debugging_Connection.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_Remote_Debugging_CheckInfo" priority="P2" purpose="Validate Crosswalk viewed webpage could be modified by remote debug" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk lanunched by remote debug mode.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_Remote_Debugging_CheckInfo.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_Remote_Debugging_Modification" priority="P2" purpose="Validate Crosswalk viewed webpage could be modified by remote debug" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure Crosswalk lanunched by remote debug mode.
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_Remote_Debugging_Modification.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_WebApp_Install_Resource" priority="P1" purpose="Validate if webapp folder is create after the webapp installed successfully" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_WebApp_Install_Resource.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_WebApp_Uninstall_Resource" priority="P1" purpose="Validate if webapp cache file is deleted after the webapp uninstalled successfully" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_WebApp_Uninstall_Resource.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="Crosswalk WRT/Package Management" execution_type="manual" id="Crosswalk_WebApp_Launcher_Cache" priority="P1" purpose="Validate if webapp isolated partition cache is created after run the app" status="approved" type="Functional">
        <description>
          <pre_condition>
            1.Make sure there some webapps are available
          </pre_condition>
          <test_script_entry>/opt/wrt-packagemgtmanu-tizen-tests/packagemgtmanu/Crosswalk_WebApp_Launcher_Cache.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
