      <testcase component="Crosswalk WRT/Core Shell" execution_type="manual" id="Crosswalk_Multiple_Tab_SamePages" purpose="Validate Crosswalk enable multiple instances for same pages">
        <description>
          <pre_condition>
            1.Make sure Crosswalk binary is installed on device;
            2.Make sure device connected to network.
          </pre_condition>
          <test_script_entry>/opt/wrt-coreshellmanu-android-tests/coreshellmanu/bdd/Crosswalk_Multiple_Tab_SamePages.html </test_script_entry>
