{
    "pkg-blacklist": [
        "config.xml",
        "pack.py",
        "testcase.xsl",
        "testresult.xsl",
        "tests.css",
        "icon.png",
        "manifest.json",
        "suite.json",
        "inst.*"
    ],
    "pkg-list": {
        "apk,cordova": {
            "blacklist": [
                "*"
