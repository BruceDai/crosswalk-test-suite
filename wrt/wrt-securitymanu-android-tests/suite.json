                    "apk-icon-opt": ""
                },
                "testapp/permission_field_contacts_tests": {
                    "apk-common-opts": "--enable-remote-debugging",
                    "apk-icon-opt": ""
                },
                "testapp/permission_field_geolocation_tests": {
                    "apk-common-opts": "--enable-remote-debugging",
